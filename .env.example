# WhaleBots environment configuration
# Sao chép file này thành .env và điền thông tin phù hợp

# Discord Bot Token (BẮT BUỘC)
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# Đường dẫn tới thư mục cài đặt WhaleBots (ví dụ trên Windows)
WHALEBOTS_PATH=C:\Path\To\WhaleBots

# Cấu hình Flask Web Dashboard
FLASK_SECRET_KEY=your_random_secret_key_here_change_this_in_production
FLASK_HOST=127.0.0.1
FLASK_PORT=5000
FLASK_DEBUG=True
